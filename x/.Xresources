! !cursor
!*cursorColor: #FFC715
!URXVT SETTINGS
!URxvt*dynamicColors: on
URxvt.internalBorder: 15
URxvt*lineSpace: 2
URxvt*scrollBar: false
URxvt*scrollColor: #131415
URxvt.geometry: 100x30
URxvt*saveLines: 1000
URxvt*cursorUnderline: false
!URxvt*cursorColor: #FFC715
URxvt*cursorBlink: true
!URxvt*underlineColor: #303030
URxvt*allow_bold: true

!PERL
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common : default,matcher

!CLICKABLE LINKS
URxvt*url-launcher: qutebrowser
URxvt*matcher.button: 1

!MISC
Xcursor*theme: 		pixelfun
Xcursor*size:  		16
XTerm*scrollBar: 	false
XTerm*rightScrollBar: 	false
XTerm*dynamicColors: true
XLogo*geometry: 	150x150
Xclock*geometry: 	150x150-0-0
Xclock*background: 	#121314

Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! Enables copy/paste with Ctrl-Shift-C/V
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

